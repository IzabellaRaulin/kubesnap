{  
   "version": 1,
   "schedule": {  
      "type": "simple",
      "interval": "1s"
   },
   "workflow": {  
      "collect": {  
         "metrics": {  
            "/intel/linux/docker/*/cpu_stats/cpu_usage/usage_in_usermode": {}
         },
         "process": null,
         "publish":[  
            {  
               "plugin_name": "heapster",
               "config": {  
                  "stats_depth": 9,
                  "server_port": 8777
               }
            }
         ]
      }
   }
}
